}
QDialog{ border: 1px 0px ; border-color: rgb(15, 15, 15); border-style: solid}

QTextBrowser{ 	color: black; 	background-color: white;}
QTextBrowser:disabled {color:gray;}

QToolButton, QPushButton {   
    /* Margins so that we get a little space on the left and right. */
    margin: 0 1px 0 1px;
    background-color:  rgba(249, 211, 176, 10%);
    /* We'll round the borders. */
    border-style: outset;
    /* Round radius will be 3px */
    border-radius: 3px;
    /* Border is only one pixel */
    border-width: 1px;
    /* Border color is now set */
    border-color: #8F653F;
	min-height: 1.1em;
	/*color: #E67A17;*/
}
#mainpage QToolButton{background-color:  rgba(249, 211, 176, 30%);}

QComboBox, QLineEdit, QSpinBox, QTabWidget, QGroupBox, QTreeWidget, QTextBrowser{
	/*background-color:  rgba(0,0,0, 0);*/
	border-style: solid;
    /* Round radius will be 3px */
    border-radius: 3px;
    /* Border is only one pixel */
    border-width: 1px;
	border-color: #8F653F;
}
QGroupBox {
margin-top: 1.5ex;
}
QGroupBox::title {
